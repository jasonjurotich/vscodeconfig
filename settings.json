{
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.suggestSelection": "first",
  "editor.fontSize": 13,
  "editor.minimap.enabled": false,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.lineNumbers": "relative",
  "editor.renderLineHighlight": "line",
  "editor.folding": false,
  "editor.glyphMargin": true,
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "editor.cursorBlinking": "solid",
  "editor.cursorWidth": 2,
  "editor.detectIndentation": false,
  "editor.hideCursorInOverviewRuler": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.formatOnType": true,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "files.trimTrailingWhitespace": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "diffEditor.wordWrap": "on",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.scrollback": 100000,
  "notebook.output.textLineLimit": 50,
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "vscode-icons",
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "git.autofetch": true,
  "git.postCommitCommand": "push",
  "git.enableSmartCommit": true,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command",
    "push:command"
  ],
  "markdown.preview.fontSize": 15,
  "indentRainbow.updateDelay": 1000,
  "vsicons.dontShowNewVersionMessage": true,
  "backgroundCover.imagePath": "/Users/jj/Documents/GERMANCASTLE.jpg",
  "settingsSync.keybindingsPerPlatform": false,
  "remoteHub.commitDirectlyWarning": "off",
  "vim.vimrc.enable": true,
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.insertModeKeyBindings": [
    {
      "before": ["w", "w"],
      "commands": [
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "print(\"\",$0);"
          }
        }
      ]
    },
    {
      "before": ["w", "u"],
      "commands": [
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "println!(\"DEBUG this is {:#?}\",$0);"
          }
        }
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.nextEditorInGroup"]
    },
    {
      "before": ["<S-tab>"],
      "commands": ["workbench.action.previousEditorInGroup"]
    },
    {
      "before": ["<S-l>"],
      "commands": ["workbench.action.navigateEditorGroups"]
    },
    {
      "before": ["<S-k>"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["leader", "g", "u"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["leader", "g", "b"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["g", "s"],
      "commands": ["cursorHome"]
    },
    {
      "before": ["g", "l"],
      "commands": ["endleft"]
    },
    {
      "before": ["<leader>", "g", "l"],
      "commands": ["endselleft"]
    },
    {
      "before": ["<leader>", "s", "v"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["<leader>", "r", "e"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["<leader>", "r", "w"],
      "commands": ["editor.action.peek"]
    },
    {
      "before": ["g", "b"],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["g", "f"],
      "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["g", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["tigs"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["togs"]
    },
    {
      "before": ["g", "v"],
      "commands": ["formsav"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.view.search"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["todo-tree-view.focus"]
    },
    {
      "before": ["S-q"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.focusSideBar"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeEditorsInGroup"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["gits"]
    },
    {
      "before": ["x"],
      "after": ["C-u"]
    },
    {
      "before": ["z"],
      "after": ["C-d"]
    },
    {
      "before": ["s", "s"],
      "after": ["Z", "Z"]
    },
    {
      "before": ["-"],
      "after": ["g", "_"]
    }
  ],
  "vim.leader": "<space>",
  "explorer.confirmDelete": false,
  "files.exclude": {
    "": true,
    "**/.git": false
  },
  "commands.commands": {
    "togs": {
      "sequence": [
        {
          "command": "workbench.action.focusActiveEditorGroup"
        },
        {
          "command": "workbench.action.toggleSidebarVisibility"
        }
      ]
    },
    "tigs": {
      "sequence": [
        {
          "command": "workbench.action.toggleSidebarVisibility"
        },
        {
          "command": "workbench.files.action.focusFilesExplorer"
        }
      ]
    },
    "formsav": {
      "sequence": [
        {
          "command": "editor.action.formatDocument"
        },
        {
          "command": "workbench.action.files.save"
        }
      ]
    },
    "endleft": {
      "sequence": [
        {
          "command": "cursorEnd"
        },
        {
          "command": "cursorLeft"
        }
      ]
    },
    "endselleft": {
      "sequence": [
        {
          "command": "cursorEndSelect"
        },
        {
          "command": "cursorLeftSelect"
        },
        {
          "command": "execCopy"
        }
      ]
    },
    "gits": {
      "sequence": [
        {
          "command": "workbench.action.terminal.sendSequence",
          "args": {
            "text": "git commit --allow-empty-message -m \"\"\n"
          }
        },
        {
          "command": "git.push"
        }
      ]
    }
  },
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "css",
    "git-commit",
    "graphql",
    "html",
    "jade",
    "jupyter",
    "latex",
    "less",
    "markdown",
    "plaintext",
    "restructuredtext",
    "scss",
    "text",
    "yaml",
    "yml"
  ],
  "editor.multiCursorModifier": "ctrlCmd",
  "rust-analyzer.rustfmt.rangeFormatting.enable": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "filePathBar.statusBarAlignment": "right",
  "spotify.showLyricsButton": false,
  "spotify.showSignInButton": false,
  "spotify.showSignOutButton": false,
  "tabnine.experimentalAutoImports": true,
  "clock.format": "ddd mmm dd yyyy HH:MM",
  "clock.alignment": "Right",
  "clock.priority": 1,
  "markdown.experimental.editor.pasteLinks.enabled": true,
  "editor.experimental.pasteActions.enabled": true,
  "editor.linkedEditing": true,
  "rust-analyzer.cargo.unsetTest": ["tokio", "tokio-macros"],
  "git.allowForcePush": true,
  "git.allowNoVerifyCommit": true,
  "git.branchProtectionPrompt": "alwaysCommit",
  "git.confirmEmptyCommits": false,
  "git.confirmForcePush": false,
  "git.confirmNoVerifyCommit": false,
  // "resmon.disk.drives": false,
  "explorer.confirmDragAndDrop": false,
  "lldb.suppressUpdateNotifications": true,
  "markdown.validate.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.enableMultiLinePasteWarning": "auto",
  "code-runner.executorMapByGlob": {
    "pom.xml": "cd $dir && mvn clean package"
  },
  "workbench.editor.enablePreview": false,
  "vscode-postgres.virtualFolders": ["functions"],
  "workbench.editor.splitInGroupLayout": "vertical",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "indentRainbow.indicatorStyle": "light",
  "workbench.colorCustomizations": {
    "statusBar.background": "#000000",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#263238",
    "statusBar.foreground": "#689ab3",
    "editor.background": "#000000",
    "tab.activeBackground": "#000000",
    "tab.inactiveBackground": "#000000",
    "titleBar.activeBackground": "#000000",
    "editorGroupHeader.tabsBackground": "#000000",
    "terminal.background": "#000000",
    "panel.background": "#000000"
  },
  "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
  "rust-analyzer.inlayHints.discriminantHints.enable": "always",
  "rust-analyzer.inlayHints.closingBraceHints.minLines": 1,
  "terminal.integrated.tabs.enabled": false,
  "workbench.editor.highlightModifiedTabs": true,
  "rust-analyzer.completion.privateEditable.enable": true,
  "rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
  "debug.allowBreakpointsEverywhere": true,
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.debug.engine": "vadimcn.vscode-lldb",
  "rust-analyzer.inlayHints.bindingModeHints.enable": true,
  "rust-analyzer.lens.references.adt.enable": true,
  "rust-analyzer.lens.references.enumVariant.enable": true,
  "rust-analyzer.lens.references.trait.enable": true,
  "rust-analyzer.highlighting.strings": false,
  "markdown.marp.enableHtml": true,
  "vim.enableNeovim": true,
  "editor.formatOnSave": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "NOTE", "DEBUG"],
  "todo-tree.highlights.useColourScheme": true,
  "todo-tree.tree.autoRefresh": true,
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "redhat.telemetry.enabled": true,
  "markdown.editor.pasteUrlAsFormattedLink.enabled": "always",
  "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "lldb.launch.expressions": "native",
  "resmon.show.battery": false,
  "resmon.show.cpufreq": false,
  "vim.replaceWithRegister": true,
  "vim.smartRelativeLine": true,
  "vim.cursorStylePerMode.replace": "line",
  "vim.cursorStylePerMode.visual": "block",
  "vim.cursorStylePerMode.visualblock": "block",
  "vim.cursorStylePerMode.visualline": "line",
  "vim.neovimUseConfigFile": true,
  "vim.neovimConfigPath": "/Users/jj/.config/nvim/init.vim",
  "[swift]": {
    "editor.defaultFormatter": "vknabel.vscode-swiftformat"
  },
  "workbench.activityBar.location": "top",
  "window.commandCenter": false,
  "background.autoInstall": false,
  "background.windowBackgrounds": ["/Users/jj/Documents/GERMANCASTLE.jpg"],
  "background.backgroundOpacity": [0.85, 0.9, 0.9, 0.9]
}
